{
    "env": {
        "browser": true,
        "es2021": true
    },
    //"parser": "@babel/eslint-parser",
    "extends": [
        "@vue/eslint-config-airbnb",
        "plugin:vue/essential",
        "plugin:vue/base",
        "eslint:recommended",
        "prettier"
    ],
    "globals": {
        "window": true,
        "document": true
    },
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "vue/multi-word-component-names": 0
    }
}
